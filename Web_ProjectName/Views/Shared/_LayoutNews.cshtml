@{
    var supplierInfo = ViewBag.SupplierInfo as M_Supplier;
}
<!doctype html>
<html class="no-js" lang="zxx">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>@ViewData["Title"]</title>
    <meta name="description" content="@ViewData["Description"]">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Facebook, Zalo, Telegram Meta Tags -->
    <meta property="og:image" content="@ViewData["ImagePreview"]" />
    <meta property="og:image:alt" content="@ViewData["Title"]" />
    <meta property="og:type" content="website" />
    <meta property="og:locale" content="vi" />
    <meta property="og:site_name" content="@supplierInfo?.name" />
    <meta property="og:title" content="@ViewData["Title"]" />
    <meta property="og:description" content="@ViewData["Description"]" />

    <!-- Twitter Meta Tags -->
    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:image" content="@ViewData["ImagePreview"]" />
    <meta name="twitter:title" content="@ViewData["Title"]" />
    <meta name="twitter:description" content="@ViewData["Description"]" />

    <link rel="shortcut icon" type="image/x-icon" href="@(supplierInfo?.faviconUrl ?? "/favicon.ico")">
    
    <!-- CSS here -->
    <link rel="stylesheet" href="~/template/aznews-master/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/template/aznews-master/assets/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/template/aznews-master/assets/css/slicknav.css">
    <link rel="stylesheet" href="~/template/aznews-master/assets/css/animate.min.css">
    <link rel="stylesheet" href="~/template/aznews-master/assets/css/magnific-popup.css">
    <link rel="stylesheet" href="~/template/aznews-master/assets/css/fontawesome-all.min.css">
    <link rel="stylesheet" href="~/template/aznews-master/assets/css/themify-icons.css">
    <link rel="stylesheet" href="~/template/aznews-master/assets/css/slick.css">
    <link rel="stylesheet" href="~/template/aznews-master/assets/css/nice-select.css">
    <link rel="stylesheet" href="~/template/aznews-master/assets/css/style.css">
    <link rel="stylesheet" href="~/template/aznews-master/assets/css/responsive.css">
    @await RenderSectionAsync("Styles", required: false)
</head>

<body>
    
    <!-- Preloader Start -->
    <div id="preloader-active">
        <div class="preloader d-flex align-items-center justify-content-center">
            <div class="preloader-inner position-relative">
                <div class="preloader-circle"></div>
                <div class="preloader-img pere-text">
                    <img src="~/template/aznews-master/assets/img/logo/logo.png" alt="">
                </div>
            </div>
        </div>
    </div>
    <!-- Preloader Start-->

    <header>
        <!-- Header Start -->
       <div class="header-area">
            <div class="main-header ">
                <div class="header-top black-bg d-none d-md-block">
                   <div class="container">
                       <div class="col-xl-12">
                            <div class="row d-flex justify-content-between align-items-center">
                                <div class="header-info-left">
                                    <ul>     
                                        <li><img src="~/template/aznews-master/assets/img/icon/header_icon1.png" alt="">@DateTime.Now.ToString("dd/MM/yyyy")</li>
                                        <li><img src="~/template/aznews-master/assets/img/icon/header_icon1.png" alt="">@DateTime.Now.ToString("dddd, dd MMMM, yyyy")</li>
                                    </ul>
                                </div>
                                <div class="header-info-right">
                                    <ul class="header-social">    
                                        <li><a href="#"><i class="fab fa-twitter"></i></a></li>
                                        <li><a href="#"><i class="fab fa-instagram"></i></a></li>
                                       <li> <a href="#"><i class="fab fa-pinterest-p"></i></a></li>
                                    </ul>
                                </div>
                            </div>
                       </div>
                   </div>
                </div>
                <div class="header-mid d-none d-md-block">
                   <div class="container">
                        <div class="row d-flex align-items-center">
                            <!-- Logo -->
                            <div class="col-xl-3 col-lg-3 col-md-3">
                                <div class="logo">
                                    <a href="/"><img src="~/template/aznews-master/assets/img/logo/logo.png" alt="@supplierInfo?.name"></a>
                                </div>
                            </div>
                            <div class="col-xl-9 col-lg-9 col-md-9">
                                <div class="header-banner f-right ">
                                    <img src="~/template/aznews-master/assets/img/hero/header_card.jpg" alt="">
                                </div>
                            </div>
                        </div>
                   </div>
                </div>
               <div class="header-bottom header-sticky">
                    <div class="container">
                        <div class="row align-items-center">
                            <div class="col-xl-10 col-lg-10 col-md-12 header-flex">
                                <!-- sticky -->
                                    <div class="sticky-logo">
                                        <a href="/"><img src="~/template/aznews-master/assets/img/logo/logo.png" alt="@supplierInfo?.name"></a>
                                    </div>
                                <!-- Main-menu -->
                                <div class="main-menu d-none d-md-block">
                                    <nav>                  
                                        <ul id="navigation">    
                                            <li><a href="/">Trang chủ</a></li>
                                            <li><a href="/tin-tuc">Tin tức</a></li>
                                            <li><a href="/tin-tuc/moi-nhat">Tin mới nhất</a></li>
                                            <li><a href="/lien-he">Liên hệ</a></li>
                                        </ul>
                                    </nav>
                                </div>
                            </div>             
                            <div class="col-xl-2 col-lg-2 col-md-4">
                                <div class="header-right-btn f-right d-none d-lg-block">
                                    <i class="fas fa-search special-tag"></i>
                                    <div class="search-box">
                                        <form action="#">
                                            <input type="text" placeholder="Tìm kiếm">
                                        </form>
                                    </div>
                                </div>
                            </div>
                            <!-- Mobile Menu -->
                            <div class="col-12">
                                <div class="mobile_menu d-block d-md-none"></div>
                            </div>
                        </div>
                    </div>
               </div>
            </div>
       </div>
        <!-- Header End -->
    </header>

    <main>
        @RenderBody()
    </main>

   <footer>
       <!-- Footer Start-->
       <div class="footer-area footer-padding fix">
            <div class="container">
                <div class="row d-flex justify-content-between">
                    <div class="col-xl-5 col-lg-5 col-md-7 col-sm-12">
                        <div class="single-footer-caption">
                            <div class="single-footer-caption">
                                <!-- logo -->
                                <div class="footer-logo">
                                    <a href="/"><img src="~/template/aznews-master/assets/img/logo/logo2_footer.png" alt="@supplierInfo?.name"></a>
                                </div>
                                <div class="footer-tittle">
                                    <div class="footer-pera">
                                        <p>@(supplierInfo?.description ?? "Website tin tức cập nhật những thông tin mới nhất, nóng hổi nhất trong ngày. Chúng tôi cam kết mang đến cho độc giả những tin tức chính xác, khách quan và đa dạng.")</p>
                                    </div>
                                </div>
                                <!-- social -->
                                <div class="footer-social">
                                    <a href="#"><i class="fab fa-twitter"></i></a>
                                    <a href="#"><i class="fab fa-instagram"></i></a>
                                    <a href="#"><i class="fab fa-pinterest-p"></i></a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-3 col-lg-3 col-md-4  col-sm-6">
                        <div class="single-footer-caption mt-60">
                            <div class="footer-tittle">
                                <h4>Newsletter</h4>
                                <p>Đăng ký nhận tin tức mới nhất</p>
                                <!-- Form -->
                                <div class="footer-form" >
                                    <div id="mc_embed_signup">
                                        <form target="_blank" action="#" method="get" class="subscribe_form relative mail_part">
                                            <input type="email" name="email" id="newsletter-form-email" placeholder="Email Address"
                                            class="placeholder hide-on-focus" onfocus="this.placeholder = ''"
                                            onblur="this.placeholder = ' Email Address '">
                                            <div class="form-icon">
                                            <button type="submit" name="submit" id="newsletter-submit"
                                            class="email_icon newsletter-submit button-contactForm"><img src="~/template/aznews-master/assets/img/logo/form-iocn.png" alt=""></button>
                                            </div>
                                            <div class="mt-10 info"></div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-3 col-lg-3 col-md-4 col-sm-6">
                        <div class="single-footer-caption mt-60">
                            <div class="footer-tittle">
                                <h4>Liên kết</h4>
                                <ul>
                                    <li><a href="/">Trang chủ</a></li>
                                    <li><a href="/tin-tuc">Tin tức</a></li>
                                    <li><a href="/tin-tuc/moi-nhat">Tin mới nhất</a></li>
                                    <li><a href="/lien-he">Liên hệ</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Footer End-->
    </footer>

    <!-- JS here -->
    <script src="~/template/aznews-master/assets/js/vendor/modernizr-3.5.0.min.js"></script>
    <script src="~/template/aznews-master/assets/js/vendor/jquery-1.12.4.min.js"></script>
    <script src="~/template/aznews-master/assets/js/popper.min.js"></script>
    <script src="~/template/aznews-master/assets/js/bootstrap.min.js"></script>
    <script src="~/template/aznews-master/assets/js/owl.carousel.min.js"></script>
    <script src="~/template/aznews-master/assets/js/slick.min.js"></script>
    <script src="~/template/aznews-master/assets/js/jquery.slicknav.min.js"></script>
    <script src="~/template/aznews-master/assets/js/wow.min.js"></script>
    <script src="~/template/aznews-master/assets/js/animated.headline.js"></script>
    <script src="~/template/aznews-master/assets/js/jquery.magnific-popup.js"></script>
    <script src="~/template/aznews-master/assets/js/gijgo.min.js"></script>
    <script src="~/template/aznews-master/assets/js/jquery.nice-select.min.js"></script>
    <script src="~/template/aznews-master/assets/js/jquery.sticky.js"></script>
    <script src="~/template/aznews-master/assets/js/contact.js"></script>
    <script src="~/template/aznews-master/assets/js/jquery.form.js"></script>
    <script src="~/template/aznews-master/assets/js/jquery.validate.min.js"></script>
    <script src="~/template/aznews-master/assets/js/mail-script.js"></script>
    <script src="~/template/aznews-master/assets/js/jquery.ajaxchimp.min.js"></script>
    <script src="~/template/aznews-master/assets/js/plugins.js"></script>
    <script src="~/template/aznews-master/assets/js/main.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>